{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "https://soundcloud.com/*",
      "https://*.soundcloud.com/*"
    ]
  },
  "permissions": [
    "core:default", 
    "core:app:default",   
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://*"
        }
      ]
    }
  ]
}
